{
  "name": "easy-templates",
  "author": "Gabriel Logan",
  "version": "0.0.11",
  "description": "easy-templates - A library for creating various templates easily and efficiently using NPX.",
  "bin": "bin/index.js",
  "types": "types",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
